{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strict": true,
    "skipLibCheck": true,
    "types": ["astro/client"]
  },
  "include": ["src/**/*", "keystatic.config.tsx"],
  "exclude": ["node_modules", "dist"]
}
