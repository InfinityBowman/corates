{
  "name": "@corates/embedpdf-solid",
  "version": "0.0.1",
  "private": true,
  "description": "SolidJS bindings for EmbedPDF",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    },
    "./engines": {
      "types": "./dist/engines/index.d.ts",
      "import": "./dist/engines/index.js"
    },
    "./plugins/document-manager": {
      "types": "./dist/plugins/document-manager/index.d.ts",
      "import": "./dist/plugins/document-manager/index.js"
    },
    "./plugins/viewport": {
      "types": "./dist/plugins/viewport/index.d.ts",
      "import": "./dist/plugins/viewport/index.js"
    },
    "./plugins/scroll": {
      "types": "./dist/plugins/scroll/index.d.ts",
      "import": "./dist/plugins/scroll/index.js"
    },
    "./plugins/render": {
      "types": "./dist/plugins/render/index.d.ts",
      "import": "./dist/plugins/render/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "license": "MIT",
  "dependencies": {
    "@embedpdf/core": "^2.1.1",
    "@embedpdf/engines": "^2.1.1",
    "@embedpdf/models": "^2.1.1",
    "@embedpdf/pdfium": "^2.1.1",
    "@embedpdf/plugin-document-manager": "^2.1.1",
    "@embedpdf/plugin-viewport": "^2.1.1",
    "@embedpdf/plugin-scroll": "^2.1.1",
    "@embedpdf/plugin-render": "^2.1.1"
  },
  "peerDependencies": {
    "solid-js": "^1.9.10"
  },
  "devDependencies": {
    "solid-js": "^1.9.10",
    "typescript": "^5.9.3"
  }
}
