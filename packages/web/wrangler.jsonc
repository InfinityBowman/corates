{
  "name": "corates",
  "compatibility_date": "2025-12-01",
  "workers_dev": true,
  "assets": {
    "directory": "./dist",
    "not_found_handling": "single-page-application",
  },
  "env": {
    "production": {
      "name": "corates",
      "routes": [{ "pattern": "app.corates.org/*", "zone_name": "corates.org" }],
      "vars": {
        "VITE_WORKER_API_URL": "https://api.corates.org",
        "VITE_LANDING_URL": "https://corates.org",
        "VITE_BASEPATH": "/",
      },
    },
  },
}
